@model List<BillApp.Domain.DateCount>

@if (Model != null)
{
<div class="row">
    @foreach (var item in Model)
    {
    <div class="col-md-3">
        <div class="alert alert-success">
            <span>@item.Day/@item.Month/@item.Year (@item.Count) Facturas</span>
        </div>
    </div>
    }
</div>
}
