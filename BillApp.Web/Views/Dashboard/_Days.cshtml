@model List<BillApp.Domain.DateCount>

@if (Model != null)
{
    foreach (var item in Model)
    {
        <div id="days" class="row">
            <div class="form-group">
                <div class="col-lg-offset-1 col-md-3"><text class="form-control border-input">@item.Day/@item.Month/@item.Year</text></div>
                <div class="col-lg-offset-1 col-md-3"><text class="form-control border-input">(@item.Count)</text></div>
            </div>
        </div>     
    }    
}
