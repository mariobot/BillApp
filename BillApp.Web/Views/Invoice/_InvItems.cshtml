@model List<BillApp.Domain.InvoiceItem>

@if (Model != null)
{
    foreach (var item in Model)
    {
<div class="row">
    <div class="form-group">
        <div class="col-lg-3"><text class="form-control">@item.item</text></div>
        <div class="col-lg-1"><text class="form-control">@item.Quanty</text></div>
        <div class="col-lg-1"><text class="form-control">@item.ValueUnit</text></div>
        <div class="col-lg-2"><text class="form-control">@item.ValueTotal</text></div>
        <div class="col-lg-2">Edit Delete</div>
    </div>
</div>

    }
}
